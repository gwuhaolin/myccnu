<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="life.notice.MyNoticeEntity" table="mynotice" schema="" catalog="weixin">
		<id name="id">
			<column name="id" sql-type="int" length="10" not-null="true"/>
		</id>
		<property name="title">
			<column name="title" sql-type="varchar"/>
		</property>
		<property name="date">
			<column name="date" sql-type="varchar" length="15"/>
		</property>
		<property name="orgUrl">
			<column name="orgUrl" sql-type="varchar"/>
		</property>
		<property name="content">
			<column name="content" sql-type="longtext" length="2147483647"/>
		</property>
		<property name="fromSite">
			<column name="fromSite" sql-type="varchar" length="50"/>
		</property>
		<property name="isOk">
			<column name="isOK" sql-type="int" length="10"/>
		</property>
	</class>
</hibernate-mapping>